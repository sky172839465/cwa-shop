{
  "name": "cwa-shop",
  "private": true,
  "version": "0.132.0",
  "scripts": {
    "dev": "VITE_CJS_IGNORE_WARNING=true vite",
    "dev:mock": "MOCK=1 npm run dev",
    "dev:mockapi": "MOCK=1 MOCK_AWS_API=1 npm run dev",
    "build": "mkdir -p dist && vite build",
    "build:gh": "BASENAME=1 MOCK=1 MOCK_AWS_API=1 npm run build",
    "lint": "eslint . --ext .js,.cjs,.jsx",
    "lint:fix": "npm run lint -- --fix",
    "preview": "npm run build && vite preview",
    "preview:gh": "MOCK=1 MOCK_AWS_API=1 npm run build && vite preview",
    "prepare": "husky install",
    "prettier": "prettier --check '**/*.{json,md,yml,sh}'",
    "prettier:fix": "prettier --write '**/*.{json,md,yml,sh}'",
    "style": "stylelint **/*.css",
    "style:fix": "npm run style -- --fix"
  },
  "dependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/typography": "^0.5.10",
    "classnames": "^2.3.2",
    "daisyui": "^3.8.2",
    "date-fns": "^2.30.0",
    "filesize": "^10.1.0",
    "formik": "^2.4.5",
    "i18next": "^23.5.1",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-resources-to-backend": "^1.1.4",
    "localforage": "^1.10.0",
    "lodash-es": "^4.17.21",
    "match-sorter": "^6.3.1",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-error-boundary": "^4.0.11",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^13.2.2",
    "react-icons": "^4.11.0",
    "react-image": "^4.1.0",
    "react-loading-skeleton": "^3.3.1",
    "react-router-dom": "^6.16.0",
    "react-slick": "^0.29.0",
    "recoil": "^0.7.7",
    "safe-await": "^2.0.0",
    "slick-carousel": "^1.8.1",
    "swr": "^2.2.4",
    "video.js": "^8.6.1",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.50.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "glob": "^10.3.10",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-sh": "^0.13.1",
    "stylelint": "^15.10.3",
    "stylelint-config-standard": "^34.0.0",
    "vite": "^5.0.5",
    "vite-plugin-mock": "^2.9.8"
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "eslint --fix"
    ],
    "**/*.{css,scss}": [
      "stylelint --fix"
    ],
    "**/*.{json,md,yml,sh}": [
      "prettier --write"
    ]
  }
}
